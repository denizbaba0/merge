{
	"name": "apt",
	"mir_pm_pairs": {
		"install <package_list>": "install <package_list>",
		"update <package_list>": "upgrade <package_list>",
		"search <package_list>": "search <package_list>",
		"remove <package_list>": "remove <package_list>",
		"--upgrade-packages": "update",
		"install <package_list> --upgrade-packages": "<!>"
	}
